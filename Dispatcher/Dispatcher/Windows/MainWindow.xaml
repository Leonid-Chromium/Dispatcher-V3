<Window x:Class="Dispatcher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Dispatcher.Windows"
        xmlns:localUC="clr-namespace:Dispatcher.UCs"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
		MinHeight="600" MinWidth="1000" WindowStartupLocation="CenterScreen">
	<Grid>
		<localUC:WorkSpaceUC1 x:Name="Workspace"/>
		<localUC:SettingsUC1 x:Name="Settings" Visibility="Collapsed"/>
	</Grid>
</Window>
